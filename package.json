{
  "name": "PC-MODEL",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "sprite": "vue-cli-service serve --mode sprite",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "compression-webpack-plugin": "^3.1.0",
    "core-js": "^3.6.4",
    "element-ui": "^2.15.6",
    "html2canvas": "^1.0.0-rc.5",
    "js-md5": "^0.7.3",
    "qrcode": "^1.4.4",
    "qrcodejs2": "0.0.2",
    "vue": "^2.6.11",
    "vue-route": "^1.5.1",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3",
    "webpack-spritesmith": "^1.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.13.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lib-flexible": "^0.3.2",
    "postcss-pxtorem": "^5.1.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "overrideBrowserslist": [
          "Android >= 4.0",
          "iOS >= 8"
        ]
      },
      "postcss-pxtorem": {
        "rootValue": 75,
        "exclude": [
          "/node_modules/vant"
        ],
        "propList": [
          "*"
        ]
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
